local MacLib = loadstring(game:HttpGet("https://github.com/biggaboy212/Maclib/releases/latest/download/maclib.txt"))()

local Window = MacLib:Window({
    Title = "Porry | https://discord.gg/R7S9tTh6Uy",
    Subtitle = "Feel free to skid.",
    Size = UDim2.fromOffset(630, 360),
    DragStyle = 1,
    DisabledWindowControls = {},
    ShowUserInfo = true,
    Keybind = Enum.KeyCode.RightControl,
    AcrylicBlur = true,
})

local globalSettings = {
    UIBlurToggle = Window:GlobalSetting({
        Name = "UI Blur",
        Default = Window:GetAcrylicBlurState(),
        Callback = function(bool)
            Window:SetAcrylicBlurState(bool)
            Window:Notify({
                Title = Window.Settings.Title,
                Description = (bool and "Enabled" or "Disabled") .. " UI Blur",
                Lifetime = 5
            })
        end,
    }),
    NotificationToggler = Window:GlobalSetting({
        Name = "Notifications",
        Default = Window:GetNotificationsState(),
        Callback = function(bool)
            Window:SetNotificationsState(bool)
            Window:Notify({
                Title = Window.Settings.Title,
                Description = (bool and "Enabled" or "Disabled") .. " Notifications",
                Lifetime = 5
            })
        end,
    }),
    ShowUserInfo = Window:GlobalSetting({
        Name = "Show User Info",
        Default = Window:GetUserInfoState(),
        Callback = function(bool)
            Window:SetUserInfoState(bool)
            Window:Notify({
                Title = Window.Settings.Title,
                Description = (bool and "Showing" or "Redacted") .. " User Info",
                Lifetime = 5
            })
        end,
    })
}

local tabGroups = {
    TabGroup1 = Window:TabGroup()
}

local tabs = {
    Main = tabGroups.TabGroup1:Tab({ Name = "Full Moon", Image = "rbxassetid://18821914323" })
    Main = tabGroups.TabGroup1:Tab({ Name = "Mirage", Image = "rbxassetid://18821914323" })
    Main = tabGroups.TabGroup1:Tab({ Name = "Darkbeard", Image = "rbxassetid://18821914323" })
    Main = tabGroups.TabGroup1:Tab({ Name = "Indra", Image = "rbxassetid://18821914323" })
    Main = tabGroups.TabGroup1:Tab({ Name = "doughking", Image = "rbxassetid://18821914323" })
    Main = tabGroups.TabGroup1:Tab({ Name = "haki color", Image = "rbxassetid://18821914323" })
    Main = tabGroups.TabGroup1:Tab({ Name = "legendary sword", Image = "rbxassetid://18821914323" })
}

local sections = {
    MainSection1 = tabs.Main:Section({ Side = "Left" }),
    MainSection2 = tabs.Main:Section({ Side = "Right" })
}

local JobId, TS
local function scrapeAPI()
    local success, response = pcall(function()
        return request({
            Url = "https://hostserver.porry.store/bloxfruit/bot/JobId/fullmoon",
            Url = "https://hostserver.porry.store/bloxfruit/bot/JobId/mirage",
            Url = "https://hostserver.porry.store/bloxfruit/bot/JobId/darkbeard",
            Url = "https://hostserver.porry.store/bloxfruit/bot/JobId/indra",
            Url = "https://hostserver.porry.store/bloxfruit/bot/JobId/doughking",
            Url = "https://hostserver.porry.store/CauTrom/HAKI_COLORS.php",
            Url = "https://hostserver.porry.store/CauTrom/LEGENDARY_SWORD.php",
            Method = "GET"
        })
    end)

    if not success then
        Window:Notify({
            Title = Window.Settings.Title,
            Description = "Error while scraping API: " .. response,
            Lifetime = 5
        })
        return "Failed"
    end

    if response.Success then
        local data = game.HttpService:JSONDecode(response.Body)

        if data.Amount and data.Amount > 0 then
            local jobIds = {}

            for _, job in ipairs(data.JobId) do
                for jobId, _ in pairs(job) do
                    table.insert(jobIds, jobId)
                end
            end
            
            TS = tick()
            return jobIds
        else
            Window:Notify({
                Title = Window.Settings.Title,
                Description = "No valid JobId found.",
                Lifetime = 5
            })
            return "Failed"
        end
    else
        Window:Notify({
            Title = Window.Settings.Title,
            Description = "API response was not successful.",
            Lifetime = 5
        })
        return "Failed"
    end
end

sections.MainSection1:Header({
    Name = "Full Moon Hopper"
    Name = "mirage Hopper"
    Name = "dark beard Hopper"
    Name = "indra Hopper"
    Name = "doughking Hopper"
    Name = "haki color hopper"
    Name = "sword Hopper"
})

local ChoosenID
local jobIds = scrapeAPI()
sections.MainSection1:Button({
    Name = "Auto Hop Server",
    Callback = function()
        if tick() - TS > 100 then
            jobIds = scrapeAPI()
            if jobIds == "Failed" then
                return Window:Notify({
                    Title = Window.Settings.Title,
                    Description = "Failed to scrape API.",
                    Lifetime = 5
                })
            end
        end
    
        spawn(function()
            for _, jobId in ipairs(jobIds) do
                game:GetService("ReplicatedStorage").__ServerBrowser:InvokeServer("teleport", jobId)
                wait(5)
            end
        end)
    end    
})

sections.MainSection1:Button({
    Name = "Hop To Choosen Server",
    Callback = function()
        if tick() - TS <= 100 then
            return game:GetService("ReplicatedStorage").__ServerBrowser:InvokeServer("teleport", ChoosenID)
        end
    
        Window:Dialog({
            Title = Window.Settings.Title,
            Description = "It seems like the JobID is outdated. Would you like to refresh the server list and then hop?",
            Buttons = {
                { Name = "Yes, refresh and TP", Callback = function()
                    jobIds = scrapeAPI()
                    if jobIds ~= "Failed" then
                        ChoosenID = jobIds[math.random(#jobIds)]
                        game:GetService("ReplicatedStorage").__ServerBrowser:InvokeServer("teleport", ChoosenID)
                    else
                        Window:Notify({
                            Title = Window.Settings.Title,
                            Description = "Failed to scrape API.",
                            Lifetime = 5
                        })
                    end
                end },
                { Name = "No, keep TP", Callback = function()
                    game:GetService("ReplicatedStorage").__ServerBrowser:InvokeServer("teleport", ChoosenID)
                end }
            }
        })
    end
})

local Dropdown
sections.MainSection1:Button({
    Name = "Refresh Server List",
    Callback = function()
        jobIds = scrapeAPI()
        if jobIds ~= "Failed" then
            JobId = jobIds[math.random(1, #jobIds)]
            Dropdown:ClearOptions()
            Dropdown:InsertOptions(jobIds)
            Dropdown:UpdateSelection(JobId)
        else
            Window:Notify({
                Title = Window.Settings.Title,
                Description = "Failed to scrape API.",
                Lifetime = 5
            })
        end
    end
})

Dropdown = sections.MainSection1:Dropdown({
    Name = "Servers",
    Multi = false,
    Required = true,
    Options = jobIds ~= "Failed" and jobIds or {},
    Default = 1,
    Callback = function(Value)
        ChoosenID = Value
    end,
}, "Dropdown")

sections.MainSection2:Paragraph({
    Header = "beta script",
    Body = "status: on."
})

sections.MainSection2:Label({
    Text = "discord: https://discord.gg/zGq3NrrT"
})

sections.MainSection2:SubLabel({
    Text = "made by: nguoitinhmuathu"
})

MacLib:SetFolder("MacLib")

Window.onUnloaded(function()
    print("Unloaded!")
end)

tabs.Main:Select()
